    "@nestjs/passport": "^6.1.0",
    "@nestjs/platform-express": "^6.7.2",
    "@typegoose/typegoose": "^6.2.1",
    "@types/bcryptjs": "^2.4.2",
    "@types/cookie-parser": "^1.4.2",
    "@types/dotenv": "^8.2.0",
    "@types/validator": "^10.11.3",
    "apollo-server-express": "^2.9.4",
    "bcryptjs": "^2.4.3",
    "class-validator": "^0.10.2",
    "cookie-parser": "^1.4.4",
    "dotenv": "^8.2.0",
